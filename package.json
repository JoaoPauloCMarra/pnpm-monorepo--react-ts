{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "JoaoPauloCMarra",
  "license": "ISC",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky install",
    "format": "prettier . --check --write",
    "typecheck": "tsc -b",
    "lint": "eslint . --fix --cache --max-warnings=0",
    "test": "jest -u --colors -o --cache --maxWorkers=50%",
    "test:all": "jest -u --colors -o --cache --all -i",
    "test:watch": "jest -u --colors -o --cache --watch --maxWorkers=25%",
    "server:dev": "pnpm --filter server dev",
    "server:preview": "pnpm --filter server preview",
    "server:build": "pnpm --filter server build",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "web:preview": "pnpm --filter web preview",
    "storybook": "pnpm --filter ui-library storybook",
    "storybook:build": "pnpm --filter ui-library build-storybook",
    "storybook:preview": "npx -y http-server ./dist/ui-library-storybook --cors",
    "dev": "npm-run-all -l -p server:dev web:dev",
    "build": "npm-run-all -l -s server:build web:build",
    "ci": "npm-run-all -l -s typecheck format lint test:all build"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "@tanstack/react-query": "^4.10.1",
    "@tanstack/react-query-devtools": "^4.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "recoil": "^0.7.5",
    "styled-components": "^5.3.6",
    "styled-normalize": "^8.0.7",
    "ui-library": "workspace:*",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@jest/types": "^29.1.2",
    "@mdx-js/react": "^2.1.3",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/client-api": "^6.5.12",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/preview-web": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/react-vite": "7.0.0-alpha.25",
    "@storybook/testing-library": "^0.0.13",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.5",
    "@swc/helpers": "^0.4.11",
    "@swc/jest": "^0.2.23",
    "@swc/register": "^0.1.10",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.8.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-styled-components": "^7.1.1",
    "nock": "^13.2.9",
    "npm-run-all": "^4.1.5",
