    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.3.0",
    "babel-plugin-styled-components": "^2.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cypress": "12",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
