    "typescript": "^4.8.4",
    "vite": "^3.1.5",
    "vite-plugin-node": "^1.0.0",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
