  "repository": {
    "url": "https://github.com/JoaoPauloCMarra/pnpm-monorepo--react-ts",
    "type": "Boilerplate"
  },
  "scripts": {
    "preinstall": "only-allow pnpm",
    "prepare": "husky install",
    "format": "prettier \"**/*.{ts,tsx,md}\" --check --write",
    "lint": "eslint . --fix --cache --max-warnings=0",
    "test": "jest -u --colors -o --cache --maxWorkers=50%",
    "test:all": "jest -u --colors -o --cache --all -i",
    "test:watch": "jest -u --colors -o --cache --watch --maxWorkers=25%",
    "server:dev": "pnpm --filter server dev",
    "server:preview": "pnpm --filter server preview",
    "server:build": "pnpm --filter server build",
    "web:dev": "pnpm --filter web dev",
    "web:dev:open": "pnpm --filter web dev --open",
    "web:build": "pnpm --filter web build",
    "web:preview": "pnpm --filter web preview",
    "storybook": "pnpm --filter ui-library storybook",
    "storybook:build": "pnpm --filter ui-library build-storybook",
    "storybook:preview": "npx -y http-server ./dist/ui-library-storybook --cors",
    "dev": "npm-run-all -l -p server:dev web:dev:open",
    "dev:e2e": "npm-run-all -l -p server:dev web:dev",
    "preview": "npm-run-all -l -p server:preview web:preview",
    "build": "npm-run-all -l -s server:build web:build storybook:build",
    "cypress:open": "pnpm cypress open",
    "cypress:run": "pnpm cypress run && cypress run --component",
    "e2e": "start-server-and-test dev:e2e http://127.0.0.1:8000 cypress:run",
    "ci": "npm-run-all -l -n -s format lint test:all server:build web:build storybook:build e2e"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@tanstack/react-query": "^4.19.1",
    "@tanstack/react-query-devtools": "^4.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5",
    "recoil": "^0.7.6",
    "styled-components": "^5.3.6",
    "styled-normalize": "^8.0.7",
    "ui-library": "workspace:*",
    "utils": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
