{
  "name": "monorepo",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "JoaoPauloCMarra",
  "license": "ISC",
  "scripts": {
    "preinstall": "npx -y only-allow pnpm",
    "prepare": "husky install",
    "typecheck": "tsc -b",
    "lint": "eslint . --fix --cache --max-warnings=0",
    "prettier": "prettier . --check --write",
    "test": "jest -u --colors -o --cache --maxWorkers=50%",
    "test:all": "jest -u --colors -o --cache --all -i",
    "test:watch": "jest -u --colors -o --cache --watch --maxWorkers=25%",
    "utils:build": "pnpm --filter utils build",
    "ui-library:build": "pnpm --filter ui-library build",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "web:preview": "pnpm --filter web preview",
    "build:all": "npm run utils:build && npm run ui-library:build && npm run web:build",
    "ci": "npm run prettier && npm run lint && npm run test:all && npm run build:all"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.7.2",
    "@tanstack/react-query-devtools": "^4.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "recoil": "^0.7.5",
    "styled-components": "^5.3.6",
    "ui-library": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@jest/types": "^29.1.0",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "^29.1.1",
    "jest-styled-components": "^7.1.1",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "react-is": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
